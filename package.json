{
  "name": "czqm-site",
  "version": "1.0.0",
  "description": "The new CZQM Website",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "nodemon server.js --ignore 'client/'",
    "dev-client": "npm run dev --prefix client",
    "server": "node server.js --ignore 'client/'",
    "build": "npm run build --prefix client",
    "preview": "npm run build && concurrently \"node server.js --ignore client\" \"npm run preview --prefix client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoldenXLence/CZQM-Site.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/GoldenXLence/CZQM-Site/issues"
  },
  "homepage": "https://github.com/GoldenXLence/CZQM-Site#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "7.1.0",
    "nodemon": "2.0.15"
  }
}
