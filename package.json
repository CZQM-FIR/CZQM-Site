{
  "name": "czqm-site",
  "version": "1.0.0",
  "description": "The new CZQM Website",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "nodemon server.js --ignore 'client/'",
    "dev-client": "npm run dev --prefix client",
    "server": "node server.js --ignore 'client/'",
    "build": "npm run build --prefix client",
    "preview": "npm run build && concurrently \"node server.js --ignore client\" \"npm run preview --prefix client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoldenXLence/CZQM-Site.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/GoldenXLence/CZQM-Site/issues"
  },
  "homepage": "https://github.com/GoldenXLence/CZQM-Site#readme",
  "dependencies": {
    "@formkit/vue": "^1.0.0-beta.7-c3de75f",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "flatpickr": "^4.6.13",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.2.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.5",
    "vue-flatpickr-component": "^9.0.6"
  },
  "devDependencies": {
    "concurrently": "7.1.0",
    "nodemon": "2.0.15"
  }
}
